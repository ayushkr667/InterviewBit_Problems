int Solution::singleNumber(const vector<int> &A) 
{
        int n = A.size();
        int result = 0;
        for (int i = 0; i < 32; i++) {
            int count = 0;
            for (int j = 0; j < n; j++) {
                if ((A[j] >> i) & 1) {
                    count++;
                }
            }
            result += ((count % 3) << i);
        }
        return result;
}
